<%- include('./partials/head') %>
<%- include('./partials/header') %>
<div class="container card shadow rounded py-5" style="margin-top: 7rem;">
  <h1 class="text-center card-title">Sign Up</h1>
  <form class="row g-3 card-body">
    <div class="col-md-12">
      <label for="inputUsername4" class="form-label">Username</label>
      <input type="text" class="form-control" id="inputUsername4">
    </div>
    <div class="col-md-6">
      <label for="inputPassword4" class="form-label">Password</label>
      <input type="password" class="form-control" id="inputPassword4">
    </div>
    <div class="col-6">
      <label for="confirmPassword" class="form-label">Confirm Password</label>
      <input type="password" class="form-control" id="confirmPassword" >
    </div>
    <div class="col-12">
      <button type="submit" class="btn btn-primary">Sign Up</button>
    </div>
  </form>
</div>
<%- include('./partials/footer') %>
  